{
  "manifest_version": 3,
  "name": "Weather Extension",
  "version": "1.0",
  "description": "chrome extension that displays local weather data",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["scripting", "tabs", "alarms", "storage"]
}
